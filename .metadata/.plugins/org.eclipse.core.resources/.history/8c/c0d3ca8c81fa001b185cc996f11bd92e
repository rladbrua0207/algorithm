package Summer2021;

import java.util.*;

public class Q1931 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		int n = sc.nextInt();

		int[] start = new int[n];
		int[] end = new int[n];
		int[] startTemp = new int[n];
		int[] endTemp = new int[n];
		int count = 0;
		int[] index = new int[n];
		int[] num = new int[n]; // start값이 같은 
		int[] arr = new int[n];
		int k = 0;

		for(int i = 0; i < n; i++) {
			start[i] = sc.nextInt();
			end[i] = sc.nextInt();
			startTemp[i] = start[i];
			endTemp[i] = end[i];
		}
		Arrays.sort(start);

		for(int i = 0; i < n; i++) {
			if(start[i] == start[i+1]) {
				count ++;
			}else{
				if(count > 0) {
					for(int j = 0; j < n; j++) {
						if(start[i] == startTemp[j]) {
							arr[k] = endTemp[j];
							k++;
						}
					}
					Arrays.sort(arr);
					for(int p = count; p >= 0; p--) {
						end[i-p] = arr[count - p];
					}

					k = 0;
					count = 0;
				}else {
					for(int j = 0; j < n; j++) {
						if(start[i] == startTemp[j]) {
							end[i] = endTemp[j];
							break;
						}
					}
				}
			}
		}
		System.out.println(start);
		System.out.println(end);
	}
}
/**
		int num = 0;
		for(int i = 0; i < n; i++) {
			for(int j = 0 ; j < n; j++) {
				end[i] 
			}
		}
	}
}


