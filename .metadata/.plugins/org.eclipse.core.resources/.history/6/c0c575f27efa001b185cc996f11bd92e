package Summer2021;

import java.util.*;

public class Q1931 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		int n = sc.nextInt();

		int[] start = new int[n];
		int[] end = new int[n];
		int[] startTemp = new int[n];
		int[] endTemp = new int[n];
		int count = 0;
		int[] index = new int[n];
		int[] num = new int[n]; // start값이 같은 
		int[] sort = new int[n];

		for(int i = 0; i < n; i++) {
			start[i] = sc.nextInt();
			end[i] = sc.nextInt();
			startTemp[i] = start[i];
			endTemp[i] = end[i];
		}
		Arrays.sort(start);

		for(int i = 0; i < n; i++) {
			if(start[i] == start[i++]) {

			}
		}

		for(int i = 0; i < n; i++) {
			for(int j = 0; j < n; j++) {
				if(start[i] == startTemp[j] && count == 0) {
					end[i] = endTemp[j];
					break;
				}
			}
		}
	}
}


