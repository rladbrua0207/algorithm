import java.util.Arrays;

public class Test {
	public static void main(String[] args) {
		int[] A = new int[6];
		Test t = new Test();
		A[0] = 1;
		A[1] = 3;
		A[2] = 6;
		A[3] = 4;
		A[4] = 7;
		A[5] = 2;
		System.out.println(t.solution(A));
	}
	public int solution(int[] A) {
		int min;
		int tmp;
		for (int i = 0; i < A.length - 1; i++) {
			min = i;
			for (int j = i + 1; j < A.length; j++) {
				if (A[j] < A[min])
					min = j;
			}
			tmp = A[i];
			A[i] = A[min];
			A[min] = tmp;
		}
		int a = 0;
		for(int i = 0; i < A.length; i++) {
			if(A[A.length-1] <=0) {
				a = 1;
			}else if(i == A.length-1) {
				a = A[i] + 1;
			}else if(i){

			}

			else if(A[i] != A[i+1]-1) {
				a=A[i] + 1;
				break;
			}
		}
	}
	return a;		

}
}
