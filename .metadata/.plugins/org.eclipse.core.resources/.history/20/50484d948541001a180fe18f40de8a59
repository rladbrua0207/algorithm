import java.util.Arrays;

public class Test {
	public static void main(String[] args) {
		int[] A = new int[5];
		Test t = new Test();
		A[0] = -1;
		A[1] = -2;
		A[2] = -3;
		A[3] = -4;
		A[4] = -5;
		System.out.println(t.solution(A));
	}
	public int solution(int[] A) {
		Arrays.sort(A);
		int a = 0;
		for(int i = 0; i < A.length; i++) {
			if(A[A.length-1] <=0) {
				a = 1;
			}else if(i == A.length-1) {
				a = A[i] + 1;
			}else if(A[i] != A[i+1]-1) {
				a=A[i] + 1;
				break;
			}
		}
		return a;
		
	}
}
